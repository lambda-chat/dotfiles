[user]
    name = lambda-chat
    email = chat.algebrique@gmail.com
[alias]
    ci = commit
    co = checkout
    cot = checkout --track
    cp = cherry-pick
    cpa = cherry-pick --abort
    cpi = cherry-pick -x
    di = diff -M -C
    dia = diff -M -C HEAD
    dic = diff --cached -M -C
    f = fetch
    git = !git
    lga = log --all --decorate --graph --oneline
    lgp = log --patch-with-stat -M -C
    lgs = log --stat -M -C
    m = merge --no-ff
    mff = merge --ff-only
    p = pull --rebase
    pff = pull --ff-only
    pu = push -u origin HEAD
    rea = rebase --abort
    resq = rebase --autosquash -i
    rec = rebase --continue
    rei = rebase -i
    reset-upst = reset --hard @{upstream}
    st = status
    sw = switch
    swd = switch --detach
